{
  "builds": [
    { "src": "layouts/index.html", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/", "dest": "/layouts/index.html" }, 
    { "src": "/layouts/(.*)", "dest": "/layouts/$1" }, 
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/img/(.*)", "dest": "/img/$1" }
  ]
}
